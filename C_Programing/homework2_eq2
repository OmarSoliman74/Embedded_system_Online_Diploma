#include "stdio.h"
void main()
{
	char x;
	printf ("Enter a character you want to check:");
	fflush (stdin); fflush (stdout);
	scanf ("%c",&x);
	if (x == 'i','e','o','a','u')
	{
		printf("%c is vowel",x);
	}
	else
	{
      printf("%c is consonant",x);
	}
}


