#include "stdio.h"
void main()
{

	int i;
	int x;
	int sum=1;
	printf("enter the integer: ");
	fflush (stdin); fflush (stdout);
	scanf("%d",&x);
	if(x<0)
	{
		printf("error : factorial of negative number doesnot exist ");
	}
	else
	{
	for (i=0,sum=1 ; i<x ; i++,sum++)
	{
		sum = sum * i;
	}
	printf("%d",sum);
	}

}


