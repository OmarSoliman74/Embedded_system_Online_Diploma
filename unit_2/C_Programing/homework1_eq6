#include "stdio.h"
void main()
{
	float a;
	float b;
	printf("enter value of a :");
	fflush (stdin); fflush (stdout);
	scanf("%f",&a);
	
	printf("enter value of b :");
	fflush (stdin); fflush (stdout);
	scanf("%f",&b);
	
	printf("after swapping , value of a = %f\n",b);
	printf("after swapping , value of b = %f",a);
	
}


