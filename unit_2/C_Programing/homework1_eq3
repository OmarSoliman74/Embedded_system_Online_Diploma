#include "stdio.h"
void main()
{
	int x;
	int y;
	int sum;
	printf("enter first integer :");
	fflush (stdin); fflush (stdout);
	scanf("%d",&x);


	printf("enter second integer :");
	fflush (stdin); fflush (stdout);
	scanf("%d",&y);
	sum = x +y ;
	printf ("the sum = %d",sum);

}


